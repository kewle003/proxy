    font-weight:normal;
    border-bottom:0;
    width:100%; 
    margin:0 2em 0
}

label.ss-choice-label{
    width:100%; 
    font-weight:normal;
    border-bottom:0
}

label.ss-other-option{
    font-weight:normal;
    border-bottom:0
}

h2.ss-section-title{
    background-color:transparent;
    padding:0.4em;
    margin:0 -0.4em 0;
    color:black;
    font-weight:bold
}

legend.ss-section-title{
    background-color:transparent;
    padding:0.4em;
    margin:0 -0.4em 0;
    color:black;
    font-weight:bold
}

fieldset.iac-form{
    border:1px solid;
    width:92%
}

div.ss-form-entry{
