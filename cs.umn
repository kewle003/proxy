#main_head{
    background-image: url('../images/cse/header.jpg');
    height: 75px; 
}

ul {
    margin: 0;
}

#unitfooter_left, #unitfooter_right {
    margin-top: 8px;
}
/* The following styles are for IAC appliation form*/

.ss-required-asterisk{
    color:#c43b1d
}

label.ss-q-title{
